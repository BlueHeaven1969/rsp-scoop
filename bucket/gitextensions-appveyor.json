{
    "homepage": "https://gitextensions.github.io/",
    "version": "3.2.0.6140",
    "license": "GPL-3.0-only",
    "description": "Git Standalone UI Tool. Installs latest appveyor build of master branch",
    "notes": "Post install script will convert app to portable mode.",
    "url": "https://ci.appveyor.com/api/buildjobs/s5a0epklig4jqrms/artifacts/Setup/GitExtensions-Portable-3.2.0.6140.zip",
    "hash": "061de62838ed086219d8673aa7985686a3e33775643b31f20ce0e2b61489bd06",
    "bin": [
        [
            "GitExtensions\\GitExtensions.exe",
            "gite"
        ]
    ],
    "shortcuts": [
        [
            "GitExtensions\\GitExtensions.exe",
            "Git Extensions"
        ]
    ],
    "checkver": {
        "url": "https://ci.appveyor.com/api/projects/gitextensions/gitextensions/branch/master",
        "regex": "\"jobId\":\"(?<buildjob>.*?)\".*\"version\":\"(?<version>[\\d.]+)\""
    },
    "autoupdate": {
        "url": "https://ci.appveyor.com/api/buildjobs/$matchBuildjob/artifacts/Setup/GitExtensions-Portable-$version.zip"
    },
    "post_install": [
        "(Get-Content -path \"$dir\\GitExtensions\\GitExtensions.exe.config\" -Raw) -replace '(?ms)IsPortable(.*?)False', 'IsPortable$1True' | Set-Content -path \"$dir\\GitExtensions\\GitExtensions.exe.config\"",
        "Copy-Item \"$persist_dir\\GitExtensionsShellEx64.dll\" \"$dir\\GitExtensions\"",
        "Copy-Item \"$persist_dir\\GitExtensionsShellEx32.dll\" \"$dir\\GitExtensions\""
    ],
    "persist": [
        "GitExtensions\\GitExtensions.settings",
        "GitExtensions\\Images",
        "GitExtensions\\Recent"
    ]
}
