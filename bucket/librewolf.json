        "Remove-Item \"$lw_name\""
    ],
    "bin": [
        [
            "librewolf-portable.exe",
            "librewolf"
        ]
    ],
    "shortcuts": [
        [
            "librewolf-portable.exe",
            "LibreWolf"
        ]
    ],
    "persist": "Profiles",
    "checkver": {
        "url": "https://gitlab.com/api/v4/projects/13852981/releases",
        "regex": "\"links\":.*?\"name\":\"librewolf-([\\d.-]+)\\.en-US\\.win64-portable\\.zip\".*?\"url\":\"(?<updurl>.*?)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchUpdurl"
            }
        }
    }
}
