    "architecture": {
        "64bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/14131/SumatraPDF-prerel-64.zip",
            "hash": "1602c361b267a2b607a5c2ad63f419ca842385a02b24f8115d1c819982b1eae5"
        },
        "32bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/14131/SumatraPDF-prerel.zip",
            "hash": "10f119416fbcd844a1e9ed907951dd319beec59e71bfa2e57dc92b689ab0c00d"
        }
    },
    "pre_install": [
        "$file = 'SumatraPDF-settings.txt'",
        "if (-not (Test-Path \"$persist_dir\\$file\")) {",
        "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
        "    $CONT = @('CheckForUpdates = false')",
        "    Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding Ascii",
        "}",
        "touch \"$dir\\SumatraPDF.exe\""
    ],
    "post_install": [
        "del \"$dir\\SumatraPDF.exe\"",
        "mv \"$dir\\SumatraPDF-prerel-$version-64.exe\" \"$dir\\SumatraPDF.exe\""
    ],
    "checkver": {
        "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/sumatralatest.js",
        "regex": "sumLatestVer = ([\\d]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/$version/SumatraPDF-prerel-64.zip"
            },
            "32bit": {
                "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/$version/SumatraPDF-prerel.zip"
            }
        }
    },
    "bin": "SumatraPDF.exe",
    "shortcuts": [
        [
            "SumatraPDF.exe",
            "SumatraPDF"
        ]
    ],
    "persist": [
        "SumatraPDF-settings.txt",
        "sumatrapdfcache"
    ]
}
