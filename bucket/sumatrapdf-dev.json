{
    "version": "14012",
    "description": "Free PDF, eBook reader.",
    "homepage": "https://www.sumatrapdfreader.org",
    "license": "GPL-3.0-only,BSD-2-Clause",
    "architecture": {
        "64bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-14012-64.exe#/SumatraPDF.exe",
            "hash": "4d0be7107ded0dc2f31b44fa148dbb24967ee4caf92559397cef91317174116b"
        },
        "32bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-14012.exe#/SumatraPDF.exe",
            "hash": "711cd30ec7cda4b1b91acc3746a95f5dc46ebbf3b63585c795236f6154f16789"
        }
    },
    "pre_install": [
        "$file = 'SumatraPDF-settings.txt'",
        "if (-not (Test-Path \"$persist_dir\\$file\")) {",
        "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
        "    $CONT = @('CheckForUpdates = false')",
        "    Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding Ascii",
        "}"
    ],
    "checkver": {
        "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/sumatralatest.js",
        "regex": "sumLatestExe.*https://(?<url>.*-([\\d]+))"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://$matchUrl-64.exe#/SumatraPDF.exe"
            },
            "32bit": {
                "url": "https://$matchUrl.exe#/SumatraPDF.exe"
            }
        }
    },
    "bin": "SumatraPDF.exe",
    "shortcuts": [
        [
            "SumatraPDF.exe",
            "SumatraPDF"
        ]
    ],
    "persist": [
        "SumatraPDF-settings.txt",
        "sumatrapdfcache"
    ]
}
