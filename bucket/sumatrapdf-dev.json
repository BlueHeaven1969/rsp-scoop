        "    $CONT = @('CheckForUpdates = false')",
        "    Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding Ascii",
        "}"
    ],
    "checkver": {
        "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/sumatralatest.js",
        "regex": "sumLatestVer = ([\\d.]+);"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerelease-$version-64.exe#/SumatraPDF.exe"
            },
            "32bit": {
                "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerelease-$version.exe#/SumatraPDF.exe"
            }
        }
    },
    "bin": "SumatraPDF.exe",
    "shortcuts": [
        [
            "SumatraPDF.exe",
            "SumatraPDF"
        ]
    ],
    "persist": [
