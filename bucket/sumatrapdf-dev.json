{
    "version": "13076",
    "description": "Free PDF, eBook reader.",
    "homepage": "https://www.sumatrapdfreader.org",
    "license": "GPL-3.0-only,BSD-2-Clause",
    "architecture": {
        "64bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-13076-64.exe#/SumatraPDF.exe",
            "hash": "919741586e5516502fa46fb6fa808efc416a08d5d22c7fc3d01476be3d710a5c"
        },
        "32bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-13076.exe#/SumatraPDF.exe",
            "hash": "cb4c7554f021e63d7432e24c210a1f4bcdb12ee784ee76ea65454994310a16e4"
        }
    },
    "pre_install": [
        "$file = 'SumatraPDF-settings.txt'",
        "if (-not (Test-Path \"$persist_dir\\$file\")) {",
        "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
        "    $CONT = @('CheckForUpdates = false')",
        "    Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding Ascii",
        "}"
    ],
    "checkver": {
        "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/sumatralatest.js",
        "regex": "sumLatestExe.*https://(?<url>.*-([\\d]+))"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://$matchUrl-64.exe#/SumatraPDF.exe"
            },
            "32bit": {
                "url": "https://$matchUrl.exe#/SumatraPDF.exe"
            }
        }
    },
    "bin": "SumatraPDF.exe",
    "shortcuts": [
        [
            "SumatraPDF.exe",
            "SumatraPDF"
        ]
    ],
    "persist": [
        "SumatraPDF-settings.txt",
        "sumatrapdfcache"
    ]
}
