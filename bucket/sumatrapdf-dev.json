{
    "version": "13933",
    "description": "Free PDF, eBook reader.",
    "homepage": "https://www.sumatrapdfreader.org",
    "license": "GPL-3.0-only,BSD-2-Clause",
    "architecture": {
        "64bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-13933-64.exe#/SumatraPDF.exe",
            "hash": "12f035b676d795f6102df9c147e8fd589d802881862484f766cad28f0436e147"
        },
        "32bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-13933.exe#/SumatraPDF.exe",
            "hash": "c963c74a7f5320ecaba8fb20e235066333a8c9ccadfd89713f6993c79ebe3e68"
        }
    },
    "pre_install": [
        "$file = 'SumatraPDF-settings.txt'",
        "if (-not (Test-Path \"$persist_dir\\$file\")) {",
        "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
        "    $CONT = @('CheckForUpdates = false')",
        "    Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding Ascii",
        "}"
    ],
    "checkver": {
        "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/sumatralatest.js",
        "regex": "sumLatestExe.*https://(?<url>.*-([\\d]+))"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://$matchUrl-64.exe#/SumatraPDF.exe"
            },
            "32bit": {
                "url": "https://$matchUrl.exe#/SumatraPDF.exe"
            }
        }
    },
    "bin": "SumatraPDF.exe",
    "shortcuts": [
        [
            "SumatraPDF.exe",
            "SumatraPDF"
        ]
    ],
    "persist": [
        "SumatraPDF-settings.txt",
        "sumatrapdfcache"
    ]
}
