{
    "version": "13068",
    "description": "Free PDF, eBook reader.",
    "homepage": "https://www.sumatrapdfreader.org",
    "license": "GPL-3.0-only,BSD-2-Clause",
    "architecture": {
        "64bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-13068-64.exe#/SumatraPDF.exe",
            "hash": "36e4107b163ec2c87de61b08b56c34b2b4f67666ffcf57f03b0de690f035036f"
        },
        "32bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-13068.exe#/SumatraPDF.exe",
            "hash": "bd79ed95a4eb8693cb6e0800b6ab00ec892f6af2b51ec5e429714b627f76cb32"
        }
    },
    "pre_install": [
        "$file = 'SumatraPDF-settings.txt'",
        "if (-not (Test-Path \"$persist_dir\\$file\")) {",
        "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
        "    $CONT = @('CheckForUpdates = false')",
        "    Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding Ascii",
        "}"
    ],
    "checkver": {
        "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/sumatralatest.js",
        "regex": "sumLatestExe.*https://(?<url>.*-([\\d]+))"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://$matchUrl-64.exe#/SumatraPDF.exe"
            },
            "32bit": {
                "url": "https://$matchUrl.exe#/SumatraPDF.exe"
            }
        }
    },
    "bin": "SumatraPDF.exe",
    "shortcuts": [
        [
            "SumatraPDF.exe",
            "SumatraPDF"
        ]
    ],
    "persist": [
        "SumatraPDF-settings.txt",
        "sumatrapdfcache"
    ]
}
