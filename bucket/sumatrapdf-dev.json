{
    "version": "13343",
    "description": "Free PDF, eBook reader.",
    "homepage": "https://www.sumatrapdfreader.org",
    "license": "GPL-3.0-only,BSD-2-Clause",
    "architecture": {
        "64bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-13343-64.exe#/SumatraPDF.exe",
            "hash": "55e3bd1389f4dbb8fec33b520844d064679765b0b4ddb915f7625cd4c204df43"
        },
        "32bit": {
            "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/prerel/SumatraPDF-prerel-13343.exe#/SumatraPDF.exe",
            "hash": "ea10978a4dd83b46e5953d77996388934714e1aed99bf959d7a1199a5ffcf8c6"
        }
    },
    "pre_install": [
        "$file = 'SumatraPDF-settings.txt'",
        "if (-not (Test-Path \"$persist_dir\\$file\")) {",
        "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
        "    $CONT = @('CheckForUpdates = false')",
        "    Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding Ascii",
        "}"
    ],
    "checkver": {
        "url": "https://kjkpubsf.sfo2.digitaloceanspaces.com/software/sumatrapdf/sumatralatest.js",
        "regex": "sumLatestExe.*https://(?<url>.*-([\\d]+))"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://$matchUrl-64.exe#/SumatraPDF.exe"
            },
            "32bit": {
                "url": "https://$matchUrl.exe#/SumatraPDF.exe"
            }
        }
    },
    "bin": "SumatraPDF.exe",
    "shortcuts": [
        [
            "SumatraPDF.exe",
            "SumatraPDF"
        ]
    ],
    "persist": [
        "SumatraPDF-settings.txt",
        "sumatrapdfcache"
    ]
}
